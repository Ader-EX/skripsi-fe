{
  "extends": ["next/core-web-vitals"],
  "plugins": ["import"],
  "rules": {
    "import/no-unresolved": "error",
    "@next/next/no-html-link-for-pages": "error",
    "import/no-case-sensitive-paths": "off"
  },
  "settings": {
    "import/resolver": {
      "alias": {
        "map": [["@", "./"]],
        "extensions": [".js", ".jsx", ".ts", ".tsx"]
      }
    }
  }
}
